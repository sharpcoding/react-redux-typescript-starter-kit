{
  "name": "rr-ts-d3-starter-kit",
  "version": "0.5.0",
  "description": "React Redux Typescript D3 Starter Kit",
  "repository": {
    "type": "git",
    "url": "git@github.com:sharpcoding/rr-ts-d3-starter-kit.git"
  },
  "scripts": {
    "postinstall": "typings install",
    "dev": "webpack-dev-server --config webpack.config.js  --open",
