    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.74",
    "@types/react": "^15.0.26",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.43",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "ts-loader": "^2.3.5",
    "typescript": "^2.7.2",
