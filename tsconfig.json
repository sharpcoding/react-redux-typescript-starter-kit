{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@state/*": ["./src/store/state/*"],
    }
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
  
}